{
  "scriptFile" : "../demo-functions-1.0.0.jar",
  "entryPoint" : "com.example.functions.rest.ProductoRestGateway.handleProductoREST",
  "bindings" : [ {
    "type" : "httpTrigger",
    "direction" : "in",
    "name" : "req",
    "route" : "api/v1/productos/{id?}",
    "methods" : [ "GET", "POST", "PUT", "DELETE" ],
    "authLevel" : "ANONYMOUS"
  }, {
    "type" : "http",
    "direction" : "out",
    "name" : "$return"
  } ]
}